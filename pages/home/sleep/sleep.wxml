<!--pages/home/sleep/sleep.wxml-->

<image class="maingif" src="{{slimeaction}}" ></image>
<view class="image">
  <image class="im" wx:for="{{imglist1}}" wx:key="{{index}}"
    src="{{item.url}}" bindtap="click" data-id="{{item.id}}"></image>
</view>
<mp-dialog title="现在不是推荐的睡觉时间，您真的要睡？" show="{{dialogShow1}}" bindbuttontap="tapDialogButton1" buttons="{{buttons1}}"></mp-dialog>
<mp-dialog title="您希望现在睡觉？" show="{{dialogShow2}}" bindbuttontap="tapDialogButton2" buttons="{{buttons2}}"></mp-dialog>